<%= provide :head do %>
  <style type="text/css">
section{margin: 20px 0; font-family:helvetica, arial, sans-serif; font-size:.81em; color: #444; line-height:1.45;}
.head{border-bottom:none!important; width:140px; height:27px; margin:0 auto !important;padding:15px 0 0 0!important; background:url(data:image/png;base64,) no-repeat 0 10px; background-size:140px 27px;}
a:link, a:active, a:hover, a:visited{text-decoration:none; color:#304e6b;}
.updates li{min-height:40px; margin:0 8px; list-style:none; border-bottom:1px dotted #ccc; padding:10px 0px;}
.updates li .text{margin-right:45px;}
.author{font-weight:bold; padding-right:3px;}
.comment{background:url(data:image/png;base64,) no-repeat 0px 12px ; background-size:16px 16px; padding-left:20px !important;}
.like{background:url(data:image/png;base64,) no-repeat 0px 12px ; background-size:16px 16px; padding-left:20px !important;}
.tag{background:url(data:image/png;base64,) no-repeat 0px 12px ; background-size:16px 16px; padding-left:20px !important;}
.popular{background:url(data:image/png;base64,) no-repeat 0px 10px ; background-size:16px 16px; padding-left:20px !important;}
.follow{background:url(data:image/png;base64,) no-repeat 0px 10px ; background-size:16px 16px; padding-left:20px !important;}
.agree{background:url(data:image/png;base64,) no-repeat 0px 10px ; background-size:16px 16px; padding-left:20px !important;}
.timestamp{color:#ccc; text-transform:uppercase; font-size:10px; font-weight:bold;line-height:1.8;}
.content{color:#333;}
.ref-img{background-color:white; border:white 2px solid; width:35px; height:35px; float:right;-moz-box-shadow: 0px 1px 3px #b5b5b5;-webkit-box-shadow: 0px 1px 3px #b5b5b5; box-shadow: 0px 1px 3px #b5b5b5;-webkit-border-radius: 2px;
-moz-border-radius: 2px; border-radius: 2px;margin:2px 2px 0 0}
.ref-usr{background-color:white; width:35px; height:35px; float:right;-moz-box-shadow: 0px 1px 3px #c2c2c2;-webkit-box-shadow: 0px 1px 3px #c2c2c2; box-shadow: 0px 1px 3px #c2c2c2;-webkit-border-radius: 20px;
-moz-border-radius: 20px; border-radius: 20px; margin:2px 3px 0 0}

@media screen and (-webkit-min-device-pixel-ratio: 2) {
.head{background:url(data:image/png;base64,) no-repeat 0 20px ;}
.comment{background:url(data:image/png;base64,) no-repeat 0px 12px ; background-size:16px 16px;}
.fav{background:url(data:image/png;base64,) no-repeat 0px 12px ;}
.like{background:url(data:image/png;base64,) no-repeat 0px 12px ;}
.tag{background:url(data:image/png;base64,) no-repeat 0px 13px ;}
.popular{background:url(data:image/png;base64,) no-repeat 0px 11px ;}
.follow{background:url(data:image/png;base64,) no-repeat 0px 12px ;}
.agree{background:url(data:image/png;base64,) no-repeat 0px 12px;}
}
  </style>
<% end %>

<article id="scroller">
  <ul class="updates">
    <li class="head"></li>
    <% @notifications.each do |n| %>
      <%= render ('updates_' + n.notifiable_type.underscore + '_content'), :n => n %>
    <% end %>
  </ul>
</article>
<script type="text/javascript">
  //var myScroll = new iScroll('scroller');
</script>
