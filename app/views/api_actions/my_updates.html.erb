<%= provide :head do %>
  <style type="text/css">
    section{margin: 0; padding:400px 0; font-family:helvetica, arial, sans-serif; font-size:.81em; color: #888; line-height:1.4;}
    a:link, a:active, a:hover, a:visited{text-decoration:none; color:#304e6b;}
    .updates li{min-height:40px; border-bottom:1px solid #e1dddd; padding:10px 8px;}
    .updates li .text{margin-right:45px;}
    .author{font-weight:bold; padding-right:3px;}
    .comment{background:url(data:image/png;base64,) no-repeat 8px 12px ; background-size:16px 16px; padding-left:30px !important;}
    .like{background:url(data:image/png;base64,) no-repeat 8px 12px ; background-size:16px 16px; padding-left:30px !important;}
    .tag{background:url(data:image/png;base64,) no-repeat 8px 13px ; background-size:16px 16px; padding-left:30px !important;}
    .popular{background:url(data:image/png;base64,) no-repeat 8px 10px ; background-size:16px 16px; padding-left:30px !important;}
    .follow{background:url(data:image/png;base64,) no-repeat 8px 10px ; background-size:16px 16px; padding-left:30px !important;}
    .agree{background:url(data:image/png;base64,) no-repeat 8px 10px ; background-size:16px 16px; padding-left:30px !important;}
    .timestamp{color:#999; text-transform:uppercase; font-size:10px; font-weight:bold;line-height:1.8;}
    .content{color:#333;}
    .ref-img{background-color:silver;border:white 2px solid; width:32px; height:32px; float:right;-moz-box-shadow: 0px 1px 3px #b5b5b5;-webkit-box-shadow: 0px 1px 3px #b5b5b5; box-shadow: 0px 1px 3px #b5b5b5;-webkit-border-radius: 2px;-moz-border-radius: 2px; border-radius: 2px;}

    @media screen and (-webkit-min-device-pixel-ratio: 2) {
    .comment{background:url(data:image/png;base64,) no-repeat 8px 12px ; background-size:16px 16px; padding-left:30px !important;}
    .like{background:url(data:image/png;base64,) no-repeat 8px 12px ;}
    .tag{background:url(data:image/png;base64,) no-repeat 8px 13px ;}
    .popular{background:url(data:image/png;base64,) no-repeat 8px 11px ;}
    .follow{background:url(data:image/png;base64,) no-repeat 8px 12px ;}
    .agree{background:url(data:image/png;base64,) no-repeat 8px 12px;}
}
  </style>
<% end %>

<article>
  <ul class="updates">
    <% @notifications.each do |n| %>
      <%= render ('updates_' + n.notifiable_type.underscore + '_content'), :n => n %>
    <% end %>
  </ul>
</article>
