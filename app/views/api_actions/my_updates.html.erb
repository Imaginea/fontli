<%= provide :head do %>
  <style type="text/css">
section{margin: 0; font-family:helvetica, arial, sans-serif; font-size:.81em; color: #444; line-height:1.45;}

a:link, a:active, a:hover, a:visited{text-decoration:none; color:#304e6b;}
.updates li{min-height:50px; margin:0; list-style:none; border-bottom:1px solid #ececec; padding:10px 5px; -webkit-tap-highlight-color:transparent; }
.updates li .text{margin-right:45px;margin-left:8px;}
.author{font-weight:bold; padding-right:3px;}
.comment{background:url(data:image/png;base64,) no-repeat 8px 12px ; background-size:16px 16px; padding-left:20px !important;}
.like{background:url(data:image/png;base64,) no-repeat 8px 12px ; background-size:16px 16px; padding-left:20px !important;}
.tag{background:url(data:image/png;base64,) no-repeat 8px 12px ; background-size:16px 16px; padding-left:20px !important;}
.popular{background:url(data:image/png;base64,) no-repeat 8px 10px ; background-size:16px 16px; padding-left:20px !important;}
.follow{background:url(data:image/png;base64,) no-repeat 8px 10px ; background-size:16px 16px; padding-left:20px !important;}
.agree{background:url(data:image/png;base64,) no-repeat 8px 10px ; background-size:16px 16px; padding-left:20px !important;}
.timestamp{color:#ccc; text-transform:uppercase; font-size:10px; font-weight:bold;line-height:1.8;margin-left:8px;}
.content{color:#333;overflow:hidden;width:95%;display:block;}
.ref-img{background-color:white; border:white 2px solid; width:40px; height:40px; float:right;-webkit-box-shadow:  0px 1px 3px #b5b5b5; box-shadow:  0px 1px 3px #b5b5b5;-webkit-border-radius: 2px;
border-radius: 2px;margin:2px 2px 0 0;}
.ref-usr{background-color:white; width:48px; height:48px; float:right;background-size:100% 100%; position:relative;}
.ref-usr img{width:46px; height:46px; padding:1px 0 0 1px;}
.ref-usr-ring{background:url(data:image/png;base64,) no-repeat; width:48px; height:48px; position:absolute; top:0; left:0; background-size:48px 48px; -webkit-tap-highlight-color:transparent; z-index:10;}

@media screen and (-webkit-min-device-pixel-ratio: 2) {

.ref-usr-ring{background:url(data:image/png;base64,) no-repeat; -webkit-tap-highlight-color:transparent; }

.comment{background:url(data:image/png;base64,) no-repeat 8px 12px ; background-size:16px 16px;}
.fav{background:url(data:image/png;base64,) no-repeat 8px 12px ;}
.like{background:url(data:image/png;base64,) no-repeat 8px 12px ;}
.tag{background:url(data:image/png;base64,) no-repeat 8px 12px ;}
.popular{background:url(data:image/png;base64,) no-repeat 8px 11px ;}
.follow{background:url(data:image/png;base64,) no-repeat 8px 12px ;}
.agree{background:url(data:image/png;base64,) no-repeat 8px 12px;}
}
  </style>
<% end %>

<article id="scroller">
  <ul class="updates">
    <% @notifications.each do |n| %>
      <%= render ('updates_' + n.notifiable_type.underscore + '_content'), :n => n %>
    <% end %>
  </ul>
</article>
