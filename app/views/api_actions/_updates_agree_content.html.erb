<% agree = n.notifiable %>
<li class="agree">
  <img class="ref-img" src='<%= agree.font.photo.url(:thumb) %>' />
  <div class="text">
    <a class="author" href="profile:<%= n.from_user_id %>"><%= n.from_user.username %></a> agreed your Font pick <span class="content"><strong>"<%= agree.font.family_name %>"</strong></span>
  </div>
  <div class="timestamp"><%= timestamp(n.updated_at) %></div>
</li>
