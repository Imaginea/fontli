<%= provide :head do %>
  <script type="application/javascript" src="/perma-assets/iscroll-lite.js"></script>
  <style type="text/css">
section{margin: 0; padding:380px 0 400px 0; font-family:helvetica, arial, sans-serif; font-size:.85em; color: #444; line-height:1.4;}
.head{width:157px; height:20px;border-bottom:none!important; margin:0 auto !important;padding:15px 0; background:url(data:image/png;base64,)no-repeat center center; background-size:152px 27px;}
a:link, a:active, a:hover, a:visited{text-decoration:none; color:#304e6b;}
.network li{min-height:40px;  margin-left:8px; border-bottom:1px dotted #ccc; padding:10px 8px;}
.author{font-weight:bold; padding-right:3px;}
.fav{background:url(data:image/png;base64,) no-repeat 0px 12px ; background-size:16px 16px; padding-left:20px !important;}
.like{background:url(data:image/png;base64,) no-repeat 0px 12px ; background-size:16px 16px; padding-left:20px !important;}
.tag{background:url(data:image/png;base64,) no-repeat 0px 12px ; background-size:16px 16px; padding-left:20px !important;}
.popular{background:url(data:image/png;base64,) no-repeat 0px 10px ; background-size:16px 16px; padding-left:20px !important;}
.follow{background:url(data:image/png;base64,) no-repeat 0px 10px ; background-size:16px 16px; padding-left:20px !important;}
.agree{background:url(data:image/png;base64,) no-repeat 0px 10px ; background-size:16px 16px; padding-left:20px !important;}
.timestamp{color:#ccc; text-transform:uppercase; font-size:10px; font-weight:bold;line-height:1.8;}
.content{color:#333;}
.ref-img-med{background-color:white;border:white 3px solid; width:70px; height:72px; float:left;-moz-box-shadow: 0px 1px 3px #b5b5b5;-webkit-box-shadow: 0px 1px 3px #b5b5b5; box-shadow: 0px 1px 3px #b5b5b5;-webkit-border-radius: 2px;
-moz-border-radius: 2px; border-radius: 2px; margin:8px 8px 0 0;}
.ref-img{background-color:white; width:35px; height:35px; float:left;-moz-box-shadow: 0px 1px 3px #999;-webkit-box-shadow: 0px 1px 3px #999; box-shadow: 0px 1px 3px #999;-webkit-border-radius: 20px;
-moz-border-radius: 20px; border-radius: 20px; margin:8px 8px 0 0}
.clearfix:after {content: "."; display: block; clear: both; visibility: hidden; line-height: 0; height: 0;}
.clearfix {display: inline-block;}
html[xmlns] .clearfix {display: block;}
* html .clearfix {height: 1%;}

@media screen and (-webkit-min-device-pixel-ratio: 2) {
.head{background:url(data:image/png;base64,) no-repeat;}
.fav{background:url(data:image/png;base64,) no-repeat 0px 12px ;}
.comment{background:url(data:image/png;base64,) no-repeat 0px 12px ;}
.like{background:url(data:image/png;base64,) no-repeat 0px 12px ;}
.tag{background:url(data:image/png;base64,) no-repeat 0px 13px ;}
.popular{background:url(data:image/png;base64,) no-repeat 0px 11px; }
.follow{background:url(data:image/png;base64,) no-repeat 0px 12px ;}
}
  </style>
<% end %>

<article id="scroller">
  <ul class="network">
    <li class="head"></li>
    <% @updates_by_user.each do |uid, feeds| %>
      <% feeds = feeds.group_by(&:klass_sym) %>
      <% feeds.each do |klass, farr| %>
        <%= render ('network_' + klass.to_s + '_content'), :uid => uid, :feeds => farr %>
      <% end %>
    <% end %>
  </ul>
</article>
<script type="text/javascript">
  var myScroll = new iScroll('scroller');
</script>
