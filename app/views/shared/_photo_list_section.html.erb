<img src="<%= f.url_medium %>">
<div class="overlap-b"></div>
<% unless section == 'profile-post'%>
  <i></i>
<% end %>
<ul class="mat1">
  <% if f.font_help? %><li class="mat1-a">SOS</li><% end %>
  <% if f.sos_approved? %><li class="mat1-b">SPOTTED</li><% end %>
</ul>
<span class="mat2">
  <%= f.most_agreed_font.display_name if f.sos_approved? %>
</span>
<span class="mat3">
  <%= truncate(@my_cmts[f.id].try(:first).try(:body), :length => 45) %>
</span>
<% liked = @my_lks.nil? || @my_lks[f.id] %>
<a href="#" class="mat4 <%= 'selected' if liked %> bg-tooltip"><label class='tooltip'><%= liked ? 'liked' : 'like' %></label></a>
<span class="mat5">
  <%= truncate(@my_cmts[f.id].try(:first).try(:body), :length => 40) %>
</span>
<% spotted = current_user.spotted_on?(f) %>
<a class="loc-img <%= 'selected' if spotted %> bg-tooltip" href="#"><label class="tooltip"><%= spotted ? 'spotted' : 'spot' %></label></a>
<span class="mat6">
  Comment...
</span>
